	<script type="text/javascript" src="/cpotools/mc/ui/js/jquery.hoverIntent.minified.js"><!--

//--></script>			  
	<script language="javascript" type="text/javascript"><!--
 //<![CDATA[
		var hiConfig = {
			sensitivity: 7, // number = sensitivity threshold (must be 1 or higher)
			interval: 100, // number = milliseconds for onMouseOver polling interval
			timeout: 100, // number = milliseconds delay before onMouseOut
			over: function() {
				loadShoppingCart();
			}, // function = onMouseOver callback (REQUIRED)
			out: function() { } // function = onMouseOut callback (REQUIRED)
		}
		
		var cartLoaded = false;
		var cartOpen = false;

		// Set cookie value for eStore Gallery/List view
		function viewStyle_SetCookie(cookieName, cookieValue) {
			document.cookie = cookieName+"="+escape(cookieValue)
                + "; path=/";
			setCategoryViewStyle();
		}
		
		// Set eStore Category view style by cookie value
		function setCategoryViewStyle() {
			var cookievalue = getCookie('categoryViewStyle');
			if (cookievalue == 'listView'){
				$('#shopCart').show();
				$('#list_view').show();
				$('#gallery_view').hide();
				$('#productAreaNarrow').width(500);
			} else {
				$('#shopCart').hide();
				$('#list_view').hide();
				$('#gallery_view').show();
				$('#productAreaNarrow').width(745);
			}
		}
		
		$(document).ready( function() {
		
			var cookievalue = getCookie('categoryViewStyle');
			if (cookievalue == '') {
				// set default view style to "galleryView"
				viewStyle_SetCookie('categoryViewStyle','galleryView');
			}
			
			if ('tpo' != 'personal') {	
				var lineItemQty;
				if (getCookie('shoppingcartqty') != ""){
					var quantity = getCookie('shoppingcartqty');
					lineItemQty="Shopping Cart (" + quantity + " items)";
				}else{
					lineItemQty="Shopping Cart (0 items)";
				}
				document.getElementById('scartQty').innerHTML=lineItemQty;		

				$('#scartQty').hoverIntent(hiConfig) 
			}
			if ('tpo' == 'personal')	
				parent.switchLanguage('en');
		});

		function loadShoppingCart() {
			//console.log("open");
			if (!cartOpen) {
				var quantity = getCookie('shoppingcartqty');
				var viewStyle = getCookie('categoryViewStyle');

				var numCarts = $("div.theWart").length;

				//!TBN! RQ24477 - Removed check to allow shopping cart drop down to work as before
				//if (viewStyle == 'galleryView') {
					// If the category view style is "galleryView", set cart widget does not exist to display the dropdown.
				//	numCarts = 0;
				//}

				// if the shopping cart widget exists, dont display the dropdown				
				if (numCarts > 1){
					return;
				}
				
				if (quantity > 0) {
					$.get("/shop/ajax/shoppingCart?segment=personal&locale=en",function(data) {
						if (data) {
							cartLoaded = true;
							//console.log(data);
							analyticsData.products = '';
							$(data).find('li.mainItem').each(function(index) {
								var productIDSource = $(this).find("img").attr("src");								
								var productIDPieces = productIDSource.split(/[/]/);
								var productID = productIDPieces[productIDPieces.length-1].replace(".jpg", "");
								//console.log('Product ID = ' + productID);
								
								var productQuantitySource = $(this).find("span.textRight").text();
								var productQuantityPieces = productQuantitySource.split(/[:]/);
								var productQuantity = $.trim(productQuantityPieces[productQuantityPieces.length - 1]);

								//console.log(quantitySource);
								//console.log('Quantity = ' + productQuantity );
								
								var productPriceSource = $(this).find("span.textLeft").text();
								var productPricePieces = productPriceSource.split(/[$]/);
								var productPrice = $.trim(productPricePieces[productPricePieces.length - 1]);
								
								/* For French currency change the , to designate cents to a . */
								cents= productPrice.match(/(,)[0-9]{2}$/g);
								if(cents){
									productPrice = productPrice.replace(cents,"." + cents);
								}
				
								/* Remove all commas */
								productPrice= productPrice.replace(/,/g,"");
								
								//console.log(PriceSource);
								//console.log('Price = ' + productPrice);
								analyticsData.products += ';' + productID + ';' + productQuantity + ';' + productPrice + ';;,';
							});
							
							analyticsData.products = analyticsData.products.substring(0, analyticsData.products.length-1);
							
							//trackAction(this,analyticsData.products,"scView");
							trackCustomEvent("scView");
							
							$('div .innerstuff').replaceWith(data);
							
							//fix for z-index issue in ie7 and ie8 compatability mode. Shopping cart appearing behind other screen objects
							if ( ($.browser.msie && $.browser.version == 7) || 
								($.browser.version == 7.0 && document.documentMode && document.documentMode == 8) )
							{
								$('#wrapper').css('position', 'relative');
								$('#scartWart').css('z-index', 101);
								$('#cpid').css('z-index', 102);
							}
							
							$('#scartWart').slideToggle('slow');							
							cartOpen = true;
							
						}
						else {
							//console.log("basket not in session, initializing...");
							initalizeBasket();
						}
					})
				} 
			}

		}

		function initalizeBasket() {
			$.get('/shop/business/basket.jsf', function(data) {
				//console.log("basket initialized");
				if (!cartLoaded) {
					//console.log("loading shopping cart");
					loadShoppingCart();
				}
			})
		}

		function closeShoppingCart() {
			//console.log("close");
			if (cartOpen) {
				$('#scartWart').slideToggle('slow');
				cartOpen = false;
			}
		}		
		

		//]]>
		
	
//--></script>

